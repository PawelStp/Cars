<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true"
                   assembly="SalonSamochodowy" namespace="SalonSamochodowy.Models">
  <class name="Zamowienie" table="Zamowienie" dynamic-update="true" >
    <cache usage="read-write"/>
    <id name="Id" column="Id_zamowienia" type="int">
      <generator class="native" />
    </id>
    <set name="DostawaList" inverse="true" cascade="save-update">
      <key column="Id_zamowienia" />
      <one-to-many class="Dostawa" ></one-to-many>
    </set>
    <many-to-one class="Samochod_fabryka" name="Samochod_fabryka" column="Id_samochodu_fabryka"></many-to-one>
    <many-to-one class="Pracownik" name="Pracownik" column="Id_pracownika"></many-to-one>
    <one-to-one name="Status_zamowienia" constrained="true" foreign-key="none" class="Status_zamowienia" />
    <property name="Id_pracownika" column="Id_pracownika" />
    <property name="Ilosc_zamowionych" column="Ilosc_zamowionych" />
    <property name="Data_zamowienia" column="Data_zamowienia" />
    <property name="Id_samochodu_fabryka" column="Id_samochodu_fabryka"/>
  </class>
</hibernate-mapping>