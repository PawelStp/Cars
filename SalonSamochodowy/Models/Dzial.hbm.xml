<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true"
                   assembly="SalonSamochodowy" namespace="SalonSamochodowy.Models">
  <class name="Dzial" table="Dzial" dynamic-update="true" >
    <cache usage="read-write"/>
    <id name="Id_dzialu" column="Id_dzialu" type="int">
      <generator class="native" />
    </id>
    <property name="Nazwa" column="Nazwa" />
    <set name="PracownikList" inverse="true" cascade="save-update">
      <key column="Id_dzialu" />
      <one-to-many class="Pracownik" ></one-to-many>
    </set>
  </class>
</hibernate-mapping>