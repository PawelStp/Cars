<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true"
                   assembly="SalonSamochodowy" namespace="SalonSamochodowy.Models">
  <class name="Fabryka" table="Fabryka" dynamic-update="true" >
    <cache usage="read-write"/>
    <id name="Id" column="Id_fabryki" type="int">
      <generator class="native" />
    </id>
    <set name="SamochodList" inverse="true" cascade="save-update">
      <key column="Id_fabryki" />
      <one-to-many class="Samochod_fabryka" ></one-to-many>
    </set>
    <property name="Nazwa" column="Nazwa" />
    <property name="Adres" column="Adres" />
    <property name="Nr_telefonu" column="Nr_telefonu" />
  </class>
</hibernate-mapping>
